  <%= simple_form_for [@course, @target] do |f| %>
    <%= f.input :name, required: true %>
    <%= f.input :description, required: true %>
    <%= f.input :score, required: true %>
    <%= f.submit 'Save', class: "btn btn-success text-light" %>
    <%= link_to 'Cancel', :back, class: 'btn btn-secondary text-light'%>
  <% end %>
