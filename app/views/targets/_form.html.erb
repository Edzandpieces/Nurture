  <%= simple_form_for [@course, @target] do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :score %>
    <%= f.submit 'Save', class: "btn btn-success text-light" %>
    <%= link_to 'Cancel', course_assignments_path(@course), class: 'btn btn-secondary text-light'%>
  <% end %>
